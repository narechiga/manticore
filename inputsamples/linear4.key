\annotations{
	M = 1 & (x-13)^2 + (y-13)^2 > 1;
	M = 2 & (x-13)^2 + (y-13)^2 > 1;
	M = 3 & (x-13)^2 + (y-13)^2 > 1;
}

\problem{
	\[ R x, y, t, s;
	R M;
	\]

	(
	(x^2 + y^2 < 0.5 & t >= 0)->
	\[(
		(? M = 1;
		{x' = -10*x + y, 
		y' = 2*x - 11*y,
		t' = 1})

		++ (?M = 2;
		{x' = -x,
		y' = -y,
		s' = 2})

		++ (?M = 3;
		{x' = -2*x,
		y' = -3*y}
		)
	)*\]( (x - 13)^2 + (y - 13)^2 > 1 )
	)

}
