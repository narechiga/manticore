
\annotations{
	(M = 1) & ball(x1,x2,x3) < 100;
}

\functions{
        \external R D(R, R);
}

\problem{
	\[ R x1, x2, x3, M;
	\](
	( x1^2 + x2^2 + x3^2 <= 1 ) ->
	\[(
	(?(M = 1); {
		x1' = -x1,
		x2' = -x2,
		x3' = -x3
		})
	++(?(M = 1); ?(x1^2 + x2^2 >= 10); x1 := x1; x2:=2*x2; x3:=3*x3 )
	++(?(M = 2); {
		x1' = -x1,
		x2' = -x2,
		x3' = 1
		} )
	++(?(M = 7); {
		x1' = 1,
		x2' = 5,
		x3' = x1
		})


	)***\]( x1^2 + x2^2 + x3^2 < 100 )
	)
}

