\functions{
	R sin(R);
}

\schemaVariables{
        \formula #postcondition;
        \formula #safety;

        \program DLFormula #hybridinvariant;
        \program DLProgram #dlprogram;

}

\rules{

HybridCut {
        \find( ==> \[ #dlprogram* \] #safety )
        "Invariant holds":
                \replacewith( ==> #dlUniversalClosure( \[#dlprogram\]true, #hybridinvariant -> \[#dlprogram\] (#hybridinvariant), false) );
        "Invariant implies safety":
                \replacewith( ==> #dlUniversalClosure(\[#dlprogram\]true, #hybridinvariant -> #safety, false )); 
        "Remaining states are safe":
                \replacewith( ==> \[ (?(! #hybridinvariant); #dlprogram; ?(! (#hybridinvariant)))* \] #safety )
        \displayname "Hybrid cut"
};

}


\problem{
	
	\[
		R x1;
		R x2;
	\]

	( x1^2 + x2^2 < 100 )->
	\[
		{ x1' = x2, x2' = -sin(x1) - x2 }

	\] (x1^2 + x2^2 < 1000 )

}
\proof{
(branch "dummy ID"
(rule "modality_split_right" (formula "1") (userinteraction) (nodenum "1"))
(rule "eliminate_variable_decl" (formula "1") (userinteraction) (nodenum "2"))
(rule "modality_split_right" (formula "1") (userinteraction) (nodenum "3"))
(rule "eliminate_variable_decl" (formula "1") (userinteraction) (nodenum "4"))
(rule "imp_right" (formula "1") (userinteraction) (nodenum "5"))
(rule "HybridCut" (formula "2") (inst "#hybridinvariant=99.5663*x1^2+44.9788*x1*x2+89.5837*x2^2  < 71.538353") (userinteraction) (nodenum "6"))
)
}
